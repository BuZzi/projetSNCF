<!DOCTYPE html>
<html>

    <head>

        <meta charset="UTF-8" />
        <title>{% block title %}projet SNCF!{% endblock %}</title>

        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bmatznerfoundation/css/foundation.min.css') }}" />
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

    </head>

    <body>

        <!-- Content -->
        {% block body %}
        {% endblock %}
        <!-- End Content -->

        <!-- Footer -->
        {% block footer %}
            <footer class="row">
                <div class="twelve columns">
                    <hr />
                    <div class="row">
                        <div class="six columns">
                            <p>&copy; Copyright</p>
                        </div>
                    </div>
                </div>
            </footer>
        {% endblock %}
        <!-- End Footer -->

        <!-- Javascript -->
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script type="text/javascript" src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            <script type="text/javascript" src="{{ asset('Resources/public/js/jquery.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/bmatznerfoundation/js/foundation.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/bmatznerfoundation/js/jquery.foundation.tabs.min.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/lpdwclient/js/app.js') }}"></script>
        {% endblock %}
        <!-- End Javascript -->

    </body>

</html>
